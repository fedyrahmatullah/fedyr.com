{
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    { "src": "/(.*)", "dest": "/api/index.php" },
    { "src": "/assets/css/(.*)", "dest": "/assets/css/$1" },
    { "src": "/assets/js/(.*)", "dest": "/assets/js/$1" },
    { "src": "/assets/data/(.*)", "dest": "/assets/data/$1" }
  ]
}
